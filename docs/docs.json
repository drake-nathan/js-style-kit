{
  "$schema": "https://mintlify.com/docs.json",
  "colors": {
    "dark": "#16A34A",
    "light": "#4ADE80",
    "primary": "#16A34A"
  },
  "favicon": "/favicon.svg",
  "footer": {
    "socials": {
      "github": "https://github.com/drake-nathan/js-style-kit"
    }
  },
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "name": "JS Style Kit",
  "navbar": {
    "links": [
      {
        "href": "https://github.com/drake-nathan/js-style-kit/issues",
        "label": "Issues"
      }
    ],
    "primary": {
      "href": "https://github.com/drake-nathan/js-style-kit",
      "label": "GitHub",
      "type": "button"
    }
  },
  "navigation": {
    "global": {
      "anchors": [
        {
          "anchor": "npm",
          "href": "https://www.npmjs.com/package/js-style-kit",
          "icon": "npm"
        },
        {
          "anchor": "GitHub",
          "href": "https://github.com/drake-nathan/js-style-kit",
          "icon": "github"
        }
      ]
    },
    "tabs": [
      {
        "groups": [
          {
            "group": "Get Started",
            "pages": ["introduction", "quickstart"]
          },
          {
            "group": "Configuration",
            "pages": [
              "docs/eslint-config",
              "docs/prettier-config",
              "docs/typescript-config",
              "docs/react-config",
              "docs/jsdoc-config"
            ]
          },
          {
            "group": "Tools",
            "pages": ["docs/cli"]
          }
        ],
        "tab": "Docs"
      }
    ]
  },
  "theme": "linden"
}
